{
    "name": "mui-form",
    "version": "1.1.9",
    "description": "ReactJS / ES6 form library with 1) Material UI 2) automated validation and feedback 3) and state management, to show submit/reset/loading buttons automatically",
    "directories": {
        "example": "example",
        "lib": "lib"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/paulshorey/mui-form.git"
    },
    "author": "Paul Shorey",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/paulshorey/mui-form/issues"
    },
    "homepage": "https://github.com/paulshorey/mui-form#readme",
    "keywords": [
        "react-component",
        "react"
    ],
    "scripts": {
        "precommit": "lint-staged",
        "prepublish": "babel --plugins transform-es2015-modules-umd src --ignore __tests__ --out-dir ./dist",
        "lint": "eslint ./src",
        "lintfix": "eslint ./src --fix",
        "test": "react-scripts test --env=jsdom"
    },
    "devDependencies": {
        "babel-cli": "^6.6.4",
        "babel-core": "^6.7.4",
        "babel-eslint": "^6.0.2",
        "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
        "babel-polyfill": "^6.7.4",
        "babel-preset-es2015": "^6.6.0",
        "babel-preset-react": "^6.5.0",
        "babel-preset-stage-2": "^6.5.0",
        "chai": "^3.5.0",
        "enzyme": "^2.9.1",
        "enzyme-adapter-react-16": "^1.1.1",
        "eslint": "^2.7.0",
        "eslint-plugin-babel": "^3.1.0",
        "eslint-plugin-react": "^4.2.3",
        "jsdom": "^8.1.0",
        "mocha": "^2.4.5",
        "nodemon": "^1.9.1",
        "react": "^16.2.0",
        "react-addons-test-utils": "^15.0.0",
        "react-dom": "^16.2.0",
        "react-scripts": "latest",
        "sinon": "^1.17.7"
    },
    "peerDependencies": {
        "react": "~0.14.8 || ^15.0.0",
        "react-dom": "~0.14.8 || ^15.0.0"
    },
    "dependencies": {
        "babel-runtime": "^6.6.1",
        "jQuery": "^1.7.4",
        "styled-components": "^2.1.2"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,json,css}": [
            "prettier --single-quote --trailing-comma es5 --print-width 100 --use-tabs --write",
            "git add"
        ]
    }
}
