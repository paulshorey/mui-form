{
    "name": "mui-form",
    "version": "1.1.14",
    "description": "ReactJS / ES6 form library with 1) Material UI 2) automated validation and feedback 3) and state management, to show submit/reset/loading buttons automatically",
    "directories": {
        "example": "example",
        "lib": "lib"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/paulshorey/mui-form.git"
    },
    "author": "Paul Shorey",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/paulshorey/mui-form/issues"
    },
    "homepage": "https://github.com/paulshorey/mui-form#readme",
    "keywords": [
        "react-component",
        "react",
        "material design",
        "form library",
        "form validation",
        "styled-components"
    ],
    "devDependencies": {
        "babel-cli": "^6.6.4",
        "babel-core": "^6.7.4",
        "babel-eslint": "^6.0.2",
        "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
        "babel-polyfill": "^6.7.4",
        "babel-preset-es2015": "^6.6.0",
        "babel-preset-react": "^6.5.0",
        "babel-preset-stage-2": "^6.5.0",
        "babel-runtime": "^6.6.1",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "enzyme-to-json": "^3.3.0",
        "eslint": "^2.7.0",
        "eslint-plugin-babel": "^3.1.0",
        "eslint-plugin-react": "^4.2.3",
        "jsdom": "^8.1.0",
        "nodemon": "^1.9.1",
        "pre-commit": "^1.2.2",
        "prettier-eslint-cli": "^4.7.0",
        "react": "^16.2.0",
        "react-addons-test-utils": "^15.0.0",
        "react-dom": "^16.2.0",
        "react-scripts": "latest",
        "react-test-renderer": "^16.2.0"
    },
    "peerDependencies": {
        "react": "^16.2.0",
        "react-dom": "^16.2.0"
    },
    "dependencies": {
        "jquery": "^3.2.1",
        "styled-components": "^2.1.2"
    },
    "scripts": {
        "prepublish": "babel --plugins transform-es2015-modules-umd src --ignore __tests__ --out-dir ./dist",
        "format": "prettier-eslint --write \"src/**/*.js\"",
        "lint": "eslint ./src",
        "lintfix": "eslint ./src --fix",
        "test": "CI=true react-scripts test --env=jsdom",
        "testing": "react-scripts test --env=jsdom"
    },
    "pre-commit": [
        "format",
        "test"
    ]
}
